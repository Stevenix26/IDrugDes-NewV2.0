{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": true
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/app/dashboard/page.tsx", "script.ts", "src/app/dashboard/users/page.jsx", "src/app/ThemeSwitcher.ts", "src/app/provider.tsx", "src/app/ThemeSwitcher.tsx", "src/app/prescriptions/page.jsx", "src/app/components/FormPost.tsx", "src/app/Blog/CreateDoc/page.tsx", "src/app/admin/prescriptionForm/page.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
